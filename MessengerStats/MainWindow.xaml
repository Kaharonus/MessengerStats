<Window x:Class="MessengerStats.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MessengerStats"
        mc:Ignorable="d"
        Title="Messenger stats" Height="639" Width="1140" MinHeight="360" MinWidth="600">
    <Grid Background="{StaticResource Background}" Loaded="Grid_Loaded">
        <Frame x:Name="MainContent" NavigationUIVisibility="Hidden" Background="Transparent" Panel.ZIndex="1" />
        <Image x:Name="MainContentOld" Panel.ZIndex="2" HorizontalAlignment="Center" VerticalAlignment="Center" />
    </Grid>
</Window>
